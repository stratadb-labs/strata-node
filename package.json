{
  "name": "stratadb",
  "version": "0.6.0",
  "description": "Node.js SDK for StrataDB - an embedded database for AI agents",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "jest",
    "universal": "napi universal"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strata-systems/strata-node.git"
  },
  "keywords": [
    "stratadb",
    "database",
    "embedded",
    "ai",
    "agents",
    "vector",
    "kv-store",
    "napi-rs"
  ],
  "author": "Strata Systems",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strata-systems/strata-node/issues"
  },
  "homepage": "https://github.com/strata-systems/strata-node#readme",
  "napi": {
    "name": "stratadb",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 16"
  },
  "publishConfig": {
    "access": "public"
  }
}
